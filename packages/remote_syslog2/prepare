#!/bin/bash

set -e -x

package="remote_syslog2"
url="https://github.com/papertrail/remote_syslog2/releases/download/v0.14/remote_syslog_linux_amd64.tar.gz"
file="remote_syslog_linux_amd64.tar.gz"

if [[ ! -s "${package}/${file}" ]] 
then
	mkdir -p ${package} 
	curl -sL "${url}" -o "${package}/${file}"
fi

