{
  "services": [
  {
    "id": "rdpg-haproxy",
    "name": "haproxy",
    "port": <%= p('haproxy.write_port') %>,
    "interval": "10s",
    "Check":
    {
      "script": "/var/vcap/jobs/rdpg-agent/bin/check ha_pb_pg",
      "Interval": "10s"
    }
  },
  {
    "id": "rdpg-pgbouncer",
    "name": "pgbouncer",
    "port": <%= p('pgbouncer.listen_port') %>,
    "interval": "10s",
    "Check":
    {
      "script": "/var/vcap/jobs/rdpg-agent/bin/check pb",
      "Interval": "10s"
    }
  },
  {
    "id": "rdpg-pgbdr",
    "name": "postgres",
    "port": <%= p('pgbdr.port') %>,
    "interval": "10s",
    "Check":
    {
      "script": "/var/vcap/jobs/rdpg-agent/bin/check pg",
      "Interval": "10s"
    }
  }]
}

