{
  "services": [
  {
    "id": "rdpg-haproxy",
    "name": "haproxy",
    "port": <%= p('haproxy.write_port') %>,
    "interval": "10s",
    "Check":
    {
      "http": "http://<%= p('rdpg_service.admin_user') %>:<%= p('rdpg_service.admin_pass') %>@127.0.0.1:<%= p('rdpg_service.admin_port') %>/health/ha_pb_pg",
      "Interval": "10s"
    }
  },
  {
    "id": "rdpg-pgbouncer",
    "name": "pgbouncer",
    "port": <%= p('pgbouncer.listen_port') %>,
    "interval": "10s",
    "Check":
    {
      "http": "http://<%= p('rdpg_service.admin_user') %>:<%= p('rdpg_service.admin_pass') %>@127.0.0.1:<%= p('rdpg_service.admin_port') %>/health/pb",
      "Interval": "10s"
    }
  },
  {
    "id": "rdpg-pgbdr",
    "name": "postgres",
    "port": <%= p('pgbdr.port') %>,
    "interval": "10s",
    "Check":
    {
      "http": "http://<%= p('rdpg_service.admin_user') %>:<%= p('rdpg_service.admin_pass') %>@127.0.0.1:<%= p('rdpg_service.admin_port') %>/health/pg",
      "Interval": "10s"
    }
  }]
}

